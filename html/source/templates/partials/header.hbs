<header class="header cell">
  <div class="header-logo logo">
    <i class="logo-icon icon icon-stb-logo"></i>
    <h1 class="logo-name">Stumptown<span class="logo-name-alt">Bear</span></h1>
  </div>

  <nav class="header-menu menu"><ul class="menu-list">
    {{#each menu.items}}
    <li><a class="menu-item" data-rel="{{key}}" href="{{url}}" title="{{title}}">{{title}}</a></li>
    {{/each}}
  </ul></nav>
</header>